#include <iostream>

using namespace std;

int main()
{
    char x;
    cin >> x;
    if(x >= 97 && x <= 122){
        cout <<"ALPHA\n"<<"IS SMALL";
    }else if(x >= 65 && x <= 90){
        cout <<"ALPHA\n"<<"IS CAPITAL";
    }else{
        cout <<"IS DIGIT";
    }
    return 0;
}

UPDATED 5/24/2023

#include <iostream>

using namespace std;

int main()
{
char X;
cin >> X;

if(X >= 48 && X<=57){
    cout << "IS DIGIT";
}else if(X >= 65 && X <= 90){
    cout << "ALPHA\nIS CAPITAL";
}else if (X >= 97 && X <=122){
    cout << "ALPHA\nIS SMALL";
}

}
