#include <iostream>

using namespace std;

int main()
{
    long long days,years=0,months=0;
    cin >> days;
    while(days >= 365 ){
        years = years + 1;
        days = days-365;
    }
    while(days >= 30){
        months = months + 1;
        days = days-30;
    }
    cout <<years<<" years\n" <<months <<" months\n"<<days<<" days";
    return 0;
}
